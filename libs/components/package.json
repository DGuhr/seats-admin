{
  "name": "components",
  "version": "1.1.0",
  "type": "module",
  "main": "./dist/components.js",
  "types": "./dist/components.d.ts",
  "exports": {
    ".": {
      "import": "./dist/components.js",
      "types": "./dist/components.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "wireit",
    "lint": "wireit",
    "storybook": "wireit",
    "build-storybook": "storybook build"
  },
  "wireit": {
    "build": {
      "command": "vite build",
      "dependencies": [
        "../../libs/client:build"
      ],
      "files": [
        "src/**",
        "package.json",
        "tsconfig.json",
        "vite.config.ts"
      ],
      "output": [
        "dist/**"
      ]
    },
    "lint": {
      "command": "eslint . --ext js,jsx,mjs,ts,tsx"
    },
    "storybook": {
      "command": "storybook dev -p 6006",
      "dependencies": [
        "../../libs/client:build"
      ]
    }
  },
  "dependencies": {
    "client": "file:../client/dist"
  },
  "peerDependencies": {
    "@patternfly/react-core": "^4.267.6",
    "@patternfly/react-icons": "^4.93.3",
    "@patternfly/react-table": "^4.112.39",
    "@rhoas/app-services-ui-components": "^2.20.7",
    "@tanstack/react-query": "4.24.10",
    "react": "^17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.3.4",
    "uri-template-lite": "22.1.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.0-rc.8",
    "@storybook/addon-interactions": "^7.0.0-rc.8",
    "@storybook/addon-links": "^7.0.0-rc.8",
    "@storybook/blocks": "^7.0.0-rc.8",
    "@storybook/react": "^7.0.0-rc.8",
    "@storybook/react-vite": "^7.0.0-rc.8",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "msw": "0.49.3",
    "msw-storybook-addon": "1.7.0",
    "prop-types": "^15.8.1",
    "react-redux": "7.2.9",
    "storybook": "^7.0.0-rc.8",
    "vite": "^4.1.4",
    "vite-plugin-checker": "^0.5.6",
    "vite-plugin-dts": "^2.1.0"
  }
}
